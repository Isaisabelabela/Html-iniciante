<meta charset = "UTF-8">

<script>
	alert("Divisao entre dois numeros")

	var n1 = 2;
	var n2 = 7;
	
	var div = n1/n2;

	//resultado real
	document.write("Resultado: "+ div + "<br>");
	//arredonda para inteiro
	document.write("Resultado: "+ Math.round(div) + "<br>");
	//aproxima em duas casas decimais
	document.write("Resultado: "+ div.toFixed(2));
	
	</script>
